{
    "name": "typo3/t3tstest",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Tester",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "Timespin\\T3tstest\\": "Classes",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Timespin\\T3tstest\\Tests\\": "Tests",
        }
    },
    "replace": {
        "typo3-ter/t3tstest": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "t3tstest"
        }
    }
}
