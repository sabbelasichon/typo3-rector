			</trans-unit>
			<trans-unit id="table.header.owner" resname="table.header.owner">
				<source>Owner</source>
			</trans-unit>
			<trans-unit id="table.header.deletedBy" resname="table.header.deletedBy">
				<source>Deleted by</source>
			</trans-unit>
			<trans-unit id="table.header.path" resname="table.header.path">
				<source>Path</source>
			</trans-unit>
